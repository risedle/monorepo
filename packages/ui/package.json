{
    "name": "@risedle/ui",
    "version": "1.4.2",
    "license": "MIT",
    "source": "src/index.ts",
    "main": "dist/cjs/index.js",
    "types": "dist/cjs/index.d.ts",
    "files": [
        "dist",
        "README.md"
    ],
    "homepage": "https://risedle.com",
    "repository": {
        "type": "git",
        "url": "git+https://github.com/risedle/monorepo.git"
    },
    "bugs": {
        "url": "https://github.com/risedle/monorepo/issues"
    },
    "author": {
        "name": "bayu",
        "email": "bayu@risedle.com",
        "url": "https://github.com/pyk"
    },
    "contributors": [],
    "scripts": {
        "prettier": "prettier --check .",
        "lint": "eslint .",
        "storybook:start": "start-storybook -p 6006",
        "storybook:build": "build-storybook",
        "build": "pnpm clean && pnpm build:esm && pnpm build:cjs",
        "build:esm": "tsc -p tsconfig.esm.json",
        "build:cjs": "tsc -p tsconfig.cjs.json",
        "clean": "rm -rf dist",
        "prepublishOnly": "pnpm build"
    },
    "prettier": "@risedle/prettier-config",
    "dependencies": {
        "@chakra-ui/react": "2.3.1",
        "@emotion/react": "11.10.4",
        "@emotion/styled": "11.10.4",
        "@risedle/types": "1.9.0",
        "framer-motion": "7.2.1",
        "react": "18.2.0"
    },
    "devDependencies": {
        "@babel/core": "7.19.0",
        "@mdx-js/react": "1.6.22",
        "@risedle/eslint-config": "0.1.2",
        "@risedle/prettier-config": "0.1.2",
        "@risedle/tsconfig": "1.0.1",
        "@storybook/addon-actions": "6.5.10",
        "@storybook/addon-docs": "6.5.10",
        "@storybook/addon-essentials": "6.5.10",
        "@storybook/addon-interactions": "6.5.10",
        "@storybook/addon-links": "6.5.10",
        "@storybook/builder-webpack4": "6.5.10",
        "@storybook/manager-webpack4": "6.5.10",
        "@storybook/react": "6.5.10",
        "@storybook/testing-library": "0.0.13",
        "@types/react": "17.0.37",
        "@types/react-dom": "17.0.11",
        "babel-loader": "8.2.5",
        "eslint": "8.23.0",
        "prettier": "2.7.1",
        "prop-types": "15.8.1",
        "react-dom": "18.2.0",
        "typescript": "4.8.3"
    },
    "overrides": {
        "@mdx-js/react": {
            "react": "$react",
            "react-dom": "$react-dom"
        }
    },
    "exports": {
        ".": {
            "import": {
                "types": "./dist/esm/index.d.ts",
                "default": "./dist/esm/index.js"
            },
            "require": {
                "types": "./dist/cjs/index.d.ts",
                "default": "./dist/cjs/index.js"
            }
        }
    },
    "publishConfig": {
        "access": "public"
    }
}
